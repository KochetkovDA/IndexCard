{
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "prettier",
        "plugin:jest/style",
        "plugin:jest/recommended",
    ],
    "plugins": [
        "jest",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "error"
        ],
        "import/no-extraneous-dependencies": [
            "error", {
               "devDependencies": true
            }
        ]
    },
    "env": {
        "browser": true,
        "jest/globals": true,
    }
}
